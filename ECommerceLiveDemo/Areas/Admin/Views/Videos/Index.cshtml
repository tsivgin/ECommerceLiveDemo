@model IEnumerable<ECommerceLiveDemo.Models.Video>

@{
    ViewData["Title"] = "Index";
}

<h1 style="margin-top: 8%;text-align: center; margin-bottom:2% ">Videolar</h1>

@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstImageLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondImageLink)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstImageLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondImageLink)
            </td>
          
        </tr>
    }
    </tbody>
</table>